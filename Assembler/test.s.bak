MAIN:
  LI R1 80
  SLL R1 R1 0
  LI R2 01
  SLL R2 R2 0
  LOOP1:
    CALL RAND
    SW R1 R0 0
    ADDIU R1 1
    BNEZ R2 LOOP1
    ADDIU R2 FF
  RET
;通用函数库

  RAND:  ;随机数发生器，将15位结果返回至寄存器R0
  ; x(n+1)=(3*x(n)+0x61B9)>>1
    DATA RANDOM_SEED 1
    SW_SP R1 0
    ADDSP 1
    LOAD_DATA RANDOM_SEED R0 0
    LI R1 62
    SLL R1 R1 0
    ADDIU R1 B9
    ADDU R0 R1 R1
    ADDU R0 R1 R1
    ADDU R0 R1 R0
    SRL R0 R0 1
    SAVE_DATA RANDOM_SEED R0 0
    ADDSP FF
    LW_SP R1 0
    RET